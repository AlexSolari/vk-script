{
    "manifest_version": 2,
    "name": "VK-Script",
    "version": "0.0.1",
    "description": "Doing some profit for vk",
    "content_scripts": [
        {
            "matches": [ "http://vk.com/*" ],
            "js": [ "jquery.min.js", "popup.js" ]
        }
    ],
    "web_accessible_resources": [ "popup.html" ],
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "64": "icon.png"
    }
}